        {
            "path": "/project-team",
            "title": "Project team",
            "components": [
                {
                    "type": "Para",
                    "content": "Tell us about the key people who will deliver this project. You must add at least one team member, and can add up to 5.",
                    "options": {},
                    "schema": {},
                    "name": "teamIntro"
                },
                {
                    "type": "MultiInputField",
                    "name": "projectTeam",
                    "title": "Project team members",
                    "hint": "",

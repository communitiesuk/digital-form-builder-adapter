{
    "startPage": "/organisation-information",
    "pages": [
        {
            "path": "/organisation-information",
            "title": "Organisation information",
            "components": [
                {
                    "type": "Para",
                    "content": "<p class=\"govuk-body govuk-!-font-weight-bold\">In this section, we'll ask about:</p>\n<ul class=\"govuk-list govuk-list--bullet\">\n<li>your organisation's name and address</li>\n<li>your organisation type and registration details</li>\n<li>the project team</li>\n</ul>",
                    "options": {},
                    "schema": {},
                    "name": "orgInfoIntro"
                }
            ],
            "next": [
                {
                    "path": "/organisation-name"
                }
            ],
            "section": "FabDefault",
            "controller": "start.js"
        },
        {
            "path": "/organisation-name",
            "title": "Organisation name",
            "components": [
                {
                    "type": "TextField",
                    "name": "orgName",
                    "title": "What is the name of your organisation?",
                    "hint": "This must match your registered legal organisation name.",
                    "options": {},
                    "schema": {}
                },
                {
                    "type": "YesNoField",
                    "name": "hasAltName",
                    "title": "Does your organisation use any other names?",
                    "hint": "For example, a trading name or abbreviation that is different from your legal name.",
                    "options": {},
                    "schema": {}
                }
            ],
            "next": [
                {
                    "path": "/alternative-names",
                    "condition": "hasAltNameYes"
                },
                {
                    "path": "/organisation-address"
                }
            ],
            "section": "FabDefault"
        },
        {
            "path": "/alternative-names",
            "title": "Alternative organisation names",
            "components": [
                {
                    "type": "TextField",
                    "name": "altName1",
                    "title": "Alternative name 1",
                    "hint": "",
                    "options": {},
                    "schema": {}
                },
                {
                    "type": "TextField",
                    "name": "altName2",
                    "title": "Alternative name 2",
                    "hint": "",
                    "options": {
                        "required": false
                    },
                    "schema": {}
                }
            ],
            "next": [
                {
                    "path": "/organisation-address"
                }
            ],
            "section": "FabDefault"
        },
        {
            "path": "/organisation-address",
            "title": "Organisation address",
            "components": [
                {
                    "type": "UkAddressField",
                    "name": "orgAddress",
                    "title": "What is your organisation's registered address?",
                    "hint": "",
                    "options": {},
                    "schema": {}
                }
            ],
            "next": [
                {
                    "path": "/organisation-type"
                }
            ],
            "section": "FabDefault"
        },
        {
            "path": "/organisation-type",
            "title": "Organisation type",
            "components": [
                {
                    "type": "RadiosField",
                    "name": "orgType",
                    "title": "What type of organisation are you?",
                    "hint": "",
                    "options": {},
                    "schema": {},
                    "list": "orgTypeList",
                    "values": {
                        "type": "listRef"
                    }
                }
            ],
            "next": [
                {
                    "path": "/charity-details",
                    "condition": "isCharity"
                },
                {
                    "path": "/cic-details",
                    "condition": "isCIC"
                },
                {
                    "path": "/local-authority-details",
                    "condition": "isLocalAuthority"
                },
                {
                    "path": "/other-organisation-details",
                    "condition": "isOther"
                }
            ],
            "section": "FabDefault"
        },
        {
            "path": "/charity-details",
            "title": "Charity registration details",
            "components": [
                {
                    "type": "TextField",
                    "name": "charityNumber",
                    "title": "What is your registered charity number?",
                    "hint": "This is the number registered with the Charity Commission. For example, 1234567.",
                    "options": {},
                    "schema": {}
                },
                {
                    "type": "DatePartsField",
                    "name": "charityRegDate",
                    "title": "When was your charity registered?",
                    "hint": "For example, 27 3 2007",
                    "options": {},
                    "schema": {}
                }
            ],
            "next": [
                {
                    "path": "/project-team"
                }
            ],
            "section": "FabDefault"
        },
        {
            "path": "/cic-details",

                    },
                    "schema": {},
                    "children": [
                        {
                            "type": "TextField",
                            "name": "teamMemberName",
                            "title": "Full name",
                            "hint": "",
                            "options": {},
                            "schema": {}
                        },
                        {
                            "type": "TextField",
                            "name": "teamMemberRole",

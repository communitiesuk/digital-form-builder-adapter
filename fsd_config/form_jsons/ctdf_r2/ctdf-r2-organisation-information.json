                            "hint": "For example, Project Manager, Finance Lead",
                            "options": {},
                            "schema": {}
                        },
                        {
                            "type": "EmailAddressField",
                            "name": "teamMemberEmail",
                            "title": "Email address",
                            "hint": "",
                            "options": {},
                            "schema": {}
                        }
                    ]
                }
            ],
            "next": [
                {
                    "path": "/check-your-answers"
                }
            ],
            "section": "FabDefault",
            "controller": "RepeatingFieldPageController",
            "options": {
                "summaryDisplayMode": {
                    "samePage": true,
                    "separatePage": false,
                    "hideRowTitles": false
                },
                "customText": {
                    "samePageTitle": "Team members added",
                    "samePageTableItemName": "team member"

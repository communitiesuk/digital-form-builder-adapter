{
  "name": "@communitiesuk/designer",
  "version": "1.0.0",
  "main": "server/index.ts",
  "scripts": {
    "watch": "NODE_ENV=development webpack",
    "production": "yarn start:prod",
    "build": "NODE_ENV=production && NODE_OPTIONS=--openssl-legacy-provider && webpack",
    "start:prod": "NODE_ENV=production nodemon dist/server.js",
    "start:local": "NODE_ENV=development PERSISTENT_BACKEND=preview ts-node-dev --inspect --respawn --transpile-only server/index.ts",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider && concurrently 'yarn watch' 'yarn start:local'",
    "lint": "yarn run eslint .",
    "test": "yarn lint && yarn test:dev && yarn jest",
    "test-cov": "yarn lint && yarn test:dev && yarn jest",
    "test:dev": "lab -T test/.transform.js test/.setup.js ./**!(node_modules)/*.test.* -S -r console -o stdout -r html -o unit-test.html -I version -l --coverage-exclude client --coverage-exclude server --coverage-exclude config.js",
    "test-lab": "lab -T test/.transform.js test/.setup.js ./**!(node_modules)/.test.* -S -v -r console -o stdout -r html -o unit-test.html -I version -l",
    "test-lab-cov": "lab -T test/.transform.js test/.setup.js ./**!(node_modules)/*.test.* -S -v -t 88 -r console -o stdout -r lcov -o test-coverage/lab/lcov.info -r html -o test-coverage/lab/unit-test.html -r junit -o test-results/lab/unit-test.xml -I version -l",
    "jest": "yarn jest:client && yarn jest:server",
    "jest:client": "jest --coverage --config=jest.client.config.js -w",
    "jest:server": "jest --coverage --config=jest.server.config.js --runInBand",
    "jest:watch": "jest --watch",
    "merge-coverage": "lcov-result-merger 'test-coverage/**/lcov.info' 'test-coverage/lcov.info'"
  },
  "author": "Communities UK",
  "license": "SEE LICENSE IN LICENSE",
  "description": "Cross government digital form builder adapter (designer)",
  "dependencies": {
    "@communitiesuk/model": "1.0.0",

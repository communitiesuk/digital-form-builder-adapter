{
  "name": "digital-form-builder-dluhc-adapter",
  "private": true,
  "workspaces": [
    "model",
    "runner",
    "designer",
    "digital-form-builder/model",
    "digital-form-builder/queue-model",
    "digital-form-builder/designer",
    "digital-form-builder/runner",
    "digital-form-builder/e2e",
    "e2e-test"
  ],
  "version": "1.0.0",
  "description": "Mono repo for communitiesuk's designer, model and runner adapter for xgovformbuilder",
  "scripts": {
    "runner": "yarn workspace @communitiesuk/runner",
    "designer": "yarn workspace @communitiesuk/designer",
    "model": "yarn workspace @communitiesuk/model",
    "e2e": "yarn workspace e2e",
    "e2e-test": "yarn workspace e2e-test",
    "submodules:init": "git submodule sync --recursive && git submodule update --init --recursive",
    "setup": "yarn run submodules:init && yarn install"
  },
  "resolutions": {
    "@xgovformbuilder/model": "link:./digital-form-builder/model",
    "@xgovformbuilder/queue-model": "link:./digital-form-builder/queue-model",
    "joi": "17.13.3",
    "@hapi/hapi": "21.4.3"
  },
  "author": "Communities UK",
  "license": "MIT"
}
